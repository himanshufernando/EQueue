            android:layout_marginLeft="100dp"
            android:fontFamily="@font/open_sans"
            android:textColor="@color/colorBlack"
            android:textSize="15sp"
            android:text="150" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
